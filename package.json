{
  "name": "popcorn-native",
  "version": "0.12.0",
  "private": true,
  "scripts": {
    "prebuild": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle",
    "build": "cd android && ./gradlew assembleProd && cd ../",
    "build-and-install": "yarn build && yarn install-on-device",
    "postinstall": "rn-nodeify --install --hack --yarn",
    "install-on-device": "adb install -r ./android/app/build/outputs/apk/prod/release/app-prod-release.apk",
    "lint": "eslint ./app/**/*.js",
    "lint:fix": "yarn lint --fix",
    "run:android": "react-native run-android --variant=devDebug",
    "start": "react-native start",
    "test": "jest"
  },
  "browser": {
    "_stream_duplex": "readable-stream/duplex",
    "_stream_passthrough": "readable-stream/passthrough",
    "_stream_readable": "readable-stream/readable",
    "_stream_transform": "readable-stream/transform",
    "_stream_writable": "readable-stream/writable",
    "console": "console-browserify",
    "constants": "constants-browserify",
    "crypto": "react-native-crypto",
